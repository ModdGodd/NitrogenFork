// package.json
{
  "name": "nitrogen",
  "version": "2.0.0",
  "description": "Nitrogen Electron App",
  "main": "index.js",
  "scripts": {
    "start": "electron .",
    "package:mac": "electron-packager . Nitrogen --platform=darwin --arch=x64 --out=dist --icon=nitrogen.icns",
    "package:win": "electron-packager . Nitrogen --platform=win32 --arch=ia32 --out=dist --icon=nitrogen.ico",
    "dist": "electron-builder"
  },
  "dependencies": {
    "axios": "1.9.0"
  },
  "devDependencies": {
    "electron": "36.1.0",
    "electron-packager": "17.1.2",
    "electron-builder": "26.0.12"
  }
}
